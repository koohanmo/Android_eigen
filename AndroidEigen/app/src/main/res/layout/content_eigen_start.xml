<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.tistory.coderkoo.androideigen.EigenStartActivity"
    tools:showIn="@layout/activity_eigen_start">

    <LinearLayout
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="150dp">

        <RelativeLayout
            android:layout_weight="10"
            android:layout_width="0dp"
            android:layout_height="150dp">

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat1_11"
                android:text="0"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true" />

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat1_10"
                android:text="0"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentStart="true" />

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat1_01"
                android:text="0"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true" />

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat1_00"
                android:text="0"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true" />


        </RelativeLayout>

        <TextView
            android:text="X"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_width="0dp"
            android:textStyle="bold"
            android:layout_height="wrap_content" />

        <RelativeLayout
            android:layout_weight="10"
            android:layout_width="0dp"
            android:layout_height="150dp">

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat2_11"
                android:text="0"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true" />

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat2_10"
                android:text="0"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentStart="true" />

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat2_01"
                android:text="0"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true" />

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat2_00"
                android:text="0"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true" />


        </RelativeLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_marginTop="30dp"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="150dp">

        <TextView
            android:text="="
            android:textSize="50dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_width="0dp"
            android:textStyle="bold"
            android:layout_height="wrap_content" />

        <RelativeLayout
            android:layout_weight="2"
            android:layout_width="0dp"
            android:layout_height="150dp">

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat3_11"
                android:hint="00"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true" />

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat3_10"
                android:hint="00"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentStart="true" />

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat3_01"
                android:hint="00"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true" />

            <android.support.v7.widget.AppCompatEditText
                android:id="@+id/mat3_00"
                android:hint="00"
                android:textSize="30dp"
                android:inputType="number"
                android:gravity="center"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true" />


        </RelativeLayout>

    </LinearLayout>

    <Button
        android:id="@+id/btn_mul"
        android:layout_gravity="center"
        android:text="Mul..!"
        android:layout_width="wrap_content"
        android:layout_height="50dp" />

</LinearLayout>
